var a = prompt("¿Qué nota sacaste en el primer examen?");

var b = prompt("¿Qué nota sacaste en el segundo examen?");

var c = prompt("¿Qué nota sacaste en el tercer examen?");

a = parseInt(a);
b = parseInt(b);
c = parseInt(c);

var d = (a + b + c)/3;

alert("Tu nota media entre esas calificaciones es de "+d+"");

