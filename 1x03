     <h1>Sintaxis Javascript</h1>
     <ul>
        <l1>Valores</l1>

<ul>
           <l1>Literales</l1>
           <script>
               alert("Hola");
           </script>
</ul>
<ul>
           <l1>Variables</l1>
           <script>
               var a = 3;
               alert(a);
           </script>
</ul>
       <l1>Operadores</l1>
       <script>
          var b = 7;
          var c = b+a;
          alert(c);

       </script>
       <l1>Expresiones</l1>
       <script>
           alert("Hola"+3);
       </script>

</ul>
