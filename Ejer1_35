<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
  </head>
  <body>
   <script>
   var b = prompt("Número inicial");
   var c = prompt("Número final");
   b = parseInt(b)
   c = parseInt(c)
if (b>c) {
           var a ;
           for (a = b ; a >= c ; a--){
        document.write("<h1>"+a+"</h1>");
       }
} else {
  var a ;
  for (a = b ; a <= c ; a++){
document.write("<h1>"+a+"</h1>");
}
}
</script>
</body>
</html>
